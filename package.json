{
  "name": "reactjs-boilerplate",
  "version": "1.0.0",
  "description": "ReactJS environmental setup + A tool to learn basics of React JS",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && cp src/index.html dist/index.html && webpack-dev-server --content-base src/ --inline --hot",
    "build:prod": "webpack -p && cp src/index.html dist/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rsingh28/ReactJS-BoilerPlate.git"
  },
  "keywords": [
    "reactJS"
  ],
  "author": "Rashmeet Singh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rsingh28/ReactJS-BoilerPlate/issues"
  },
  "homepage": "https://github.com/rsingh28/ReactJS-BoilerPlate#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "webpack": "^3.9.1",
    "webpack-dev-server": "^2.9.5"
  }
}
